    <Grid Name="MainGrid" Background="{StaticResource BrushMainGrid}">
        <Grid.RowDefinitions>
            <RowDefinition Height="20"/>
            <RowDefinition Height="40"/>
            <RowDefinition Height="60"/>
            <RowDefinition Height="40"/>
            <RowDefinition />
            <RowDefinition Height="40"/>
            
        </Grid.RowDefinitions>

        <Menu IsMainMenu="True" Grid.Row="0">
            <MenuItem Name="menuFile" Header="File">
                <MenuItem Name="menuFile_1" Header="Open File" Click="menuFile_1_Click"/>
                <MenuItem Name="menuFile_2" Header="Exit" Click="menuFile_2_Click"/>
            </MenuItem>
            
            <MenuItem Name="menuFolders" Header="Folders">
                <MenuItem Name="menuFolders_1" Header="Open current Appfolder" Click="menuFolders_1_Click"/>
                <MenuItem Name="menuFolders_2" Header="Open Log folder" Click="menuFolders_2_Click"/>
                <MenuItem Name="menuFolders_3" Header="Open SendTo folder" Click="menuFolders_3_Click"/>
            </MenuItem>
            <MenuItem Name="menuHelp" Header="Help">
                <MenuItem Name="menuHelp_About" Header="About" Click="menuAbout_Click"></MenuItem>
                <MenuItem Name="menuHelp_Help" Header="Help" Click="menuHelp_Click"></MenuItem>
                <MenuItem Name="menuHelp_Links" Header="Links" Click="menuLinks_Click"></MenuItem>
            </MenuItem>
        </Menu>

        <DockPanel Grid.Row="1" LastChildFill="True" Margin="4,0,4,0">
            <StackPanel DockPanel.Dock="Left" Orientation="Horizontal">
                <Image Source="{StaticResource 1Image}" Height="20"/>
                <Label VerticalAlignment="Center" FontWeight="Bold" Foreground="{StaticResource BrushTextOnDark}">Select Target File:</Label>
            </StackPanel>
            

            <Button Name="btnOpenFile" Content="..." Height="20" Width="30"  Click="btnOpenFile_Click" DockPanel.Dock="Right"/>
            <TextBox Name="txtBoxTargetFile" VerticalAlignment="Center" FontWeight="Bold" FontSize="14">testDummy.exe</TextBox>
        </DockPanel>


        <DockPanel Grid.Row="2" LastChildFill="True" Margin="4,0,4,0">
            <StackPanel Orientation="Horizontal">
                <Label Name="lblModifications" FontSize="16" FontWeight="Bold" VerticalAlignment="Center" Foreground="#FFCBE164">Modifications:</Label>
                <Button Name="btnAnalyze" Width="60" Height="60" Background="{StaticResource BrushAnalyze}" Click="btnAnalyze_Click" FontWeight="Bold" Margin="0,0,4,0">
                    <StackPanel Orientation="Vertical" >
                        <Image Source="{StaticResource 2Image}" Height="20"/>
                        <Label Name="btnAnalyzeLabel1">Analyze</Label>
                    </StackPanel>
                </Button>
                <Button Name="btnInject" Width="60" Height="60" Background="{StaticResource BrushInject}" Click="btnInject_Click" FontWeight="Bold" Margin="0,0,4,0">
                    <StackPanel Orientation="Vertical" >
                        <Image Source="{StaticResource 3Image}" Height="20"/>
                        <Label Name="btnInjectLabel1">Inject</Label>
                    </StackPanel>
                </Button>

                <Label Name="lblAnalyze" FontSize="16" FontWeight="Bold" VerticalAlignment="Center" Foreground="#FFCBE164">Analyze:</Label>
                <Button Name="btnRun" Width="60" Height="60" Background="{StaticResource BrushRun}" Click="btnRun_Click" FontWeight="Bold" Margin="0,0,4,0">
                    <StackPanel Orientation="Vertical" >
                        <Label Name="btnRunLabel1" HorizontalAlignment="Center">Run</Label>
                        <Label Name="btnRunLabel2" HorizontalAlignment="Center">+ Trace</Label>
                    </StackPanel>
                </Button>
                <ToggleButton Name="btnTrace" Width="60" Height="60" Background="{StaticResource BrushTrace}" Click="btnTrace_Click" FontWeight="Bold" Margin="0,0,4,0">
                    <StackPanel Orientation="Vertical" >
                        <Label Name="btnTraceLabel1" HorizontalAlignment="Center">Trace</Label>
                        <Label Name="btnTraceLabel2" HorizontalAlignment="Center">Start</Label>
                    </StackPanel>
                </ToggleButton>

                <ToggleButton Name="btnFUSION" Width="60" Height="60" Background="{StaticResource BrushFUSION}" Click="btnFUSION_Click" FontWeight="Bold" Margin="0,0,4,0">
                    <StackPanel Orientation="Vertical" ToolTip="Enable/Disable FUSION logging">
                        <Label Name="btnFUSIONLabel1" HorizontalAlignment="Center">FUSION</Label>
                        <Label Name="btnFUSIONLabel2" HorizontalAlignment="Center">Start</Label>
                    </StackPanel>
                </ToggleButton>

            </StackPanel>
            
        </DockPanel>


        <DockPanel Name="dockPanMessages" Grid.Row="5" LastChildFill="True" Margin="4,0,4,0">
            <TextBox Name="txtBoxMessages" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto" DockPanel.Dock="Top" />
        </DockPanel>

        <DockPanel Name="dockPanOther" Grid.Row="3" LastChildFill="False" Margin="4,0,4,0">
            <StackPanel Orientation="Horizontal" DockPanel.Dock="Left">
                <Label Name="SelectedItemCountLabel1" Foreground="{StaticResource BrushTextOnDark}" FontSize="16" FontWeight="Bold" VerticalAlignment="Center"  Content="Selected MethodDefinitions: " />
                <Label Name="SelectedItemCountLabel2" Foreground="{StaticResource BrushTextOnDark}" FontSize="16" FontWeight="Bold" VerticalAlignment="Center"  Content="{Binding SelectedItems.Count, ElementName=lstBoxAnalyze}" />
            </StackPanel>
        </DockPanel>

        <DockPanel Name="dockPanAnalyzer" Grid.Row="4" LastChildFill="True" Margin="4,0,4,0">
            <Expander Name="ExpanderTextView" DockPanel.Dock="Left" Header="TextView" Foreground="{StaticResource BrushTextOnDark}">
                <TextBox Name="txtBoxAnalyzer" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto" Text="{Binding TextView}" />
                </Expander>

            <Expander Name="ExpanderTrace" DockPanel.Dock="Right" Header="Trace" Foreground="{StaticResource BrushTextOnDark}">
                <DockPanel Name="DockPanelTrace">
                    <StackPanel Orientation="Horizontal" DockPanel.Dock="top" Height="40">
                        <Label Name="lblTraceFilter" FontSize="16" FontWeight="Bold" VerticalAlignment="Center" Foreground="#FFCBE164">Filters:</Label>

                        <ToggleButton Name="toggleTracePID" Click="toggleTracePID_Click" Background="{StaticResource BrushPID}" Margin="0,0,4,0">
                            <TextBlock VerticalAlignment="Center">PID: *</TextBlock>
                        </ToggleButton>

                        <Label Name="lblTraceActions" FontSize="16" FontWeight="Bold" VerticalAlignment="Center" Foreground="#FFCBE164">Actions:</Label>
                        <Button Name="btnCopyToClipboard" Background="{StaticResource BrushGray}" Click="btnCopyToClipboard_Click" Margin="0,0,4,0">
                            <StackPanel Orientation="Horizontal">
                                <Image Source="{StaticResource CopyImage}"/>
                                <TextBlock VerticalAlignment="Center">Copy</TextBlock>
                            </StackPanel>
                        </Button>
                    </StackPanel>

                    <ListView Name="listview" ItemsSource="{Binding Source=TSpy.Queue}" VirtualizingStackPanel.IsVirtualizing="False">
                        <ListView.View>
                            <GridView>
                                <GridViewColumn DisplayMemberBinding="{Binding Path=Index}" Header="Index" />
                                <GridViewColumn DisplayMemberBinding="{Binding Path=Pid}" Header="Pid" />
                                <GridViewColumn DisplayMemberBinding="{Binding Path=ProcessName}" Header="ProcessName" />
                                <GridViewColumn DisplayMemberBinding="{Binding Path=Text}" Header="Text" />
                                <GridViewColumn DisplayMemberBinding="{Binding Path=Ticks}" Header="Ticks" />
                            </GridView>
                        </ListView.View>
                    </ListView>
                </DockPanel>
            </Expander>

            <Expander Name="ExpanderAnalyze" IsExpanded="True" DockPanel.Dock="Right" Header="Analyze" Foreground="{StaticResource BrushTextOnDark}">
                <DockPanel Name="DockPanelAnalayze">
                    <StackPanel Orientation="Horizontal" DockPanel.Dock="top" Height="40">
                        
                        <Label Name="lblAnalyzeActions" FontSize="16" FontWeight="Bold" VerticalAlignment="Center" Foreground="#FFCBE164">Actions:</Label>
                        
                        <Button Name="btnSelectAllMethodDefinitions" DockPanel.Dock="Top" Click="btnSelectAllMethodDefinitions_Click">
                            <StackPanel Orientation="Horizontal" Background="{StaticResource BrushMethod}">
                                <Image Source="{StaticResource MeImage}"/>
                                <TextBlock VerticalAlignment="Center">Select All</TextBlock>
                            </StackPanel>
                        </Button>



                        <Label Name="lblAnalyzeFilter" FontSize="16" FontWeight="Bold" VerticalAlignment="Center" Foreground="#FFCBE164">Filters:</Label>

                        <ToggleButton Name="toggleModules" Click="toggleModules_Click" Width="80" >
                        <StackPanel Orientation="Horizontal" Background="{StaticResource BrushModule}" >
                            <Image Source="{StaticResource MoImage}"/>
                            <TextBlock VerticalAlignment="Center">Modules</TextBlock>
                        </StackPanel>
                    </ToggleButton>
                        <ToggleButton Name="toggleTypes" Click="toggleTypes_Click" >
                        <StackPanel Orientation="Horizontal" Background="{StaticResource BrushType}">
                            <Image Source="{StaticResource TyImage}"/>
                            <TextBlock VerticalAlignment="Center">Types</TextBlock>
                        </StackPanel>
                    </ToggleButton>
                        <ToggleButton Name="toggleMethods" Click="toggleMethods_Click" >
                        <StackPanel Orientation="Horizontal" Background="{StaticResource BrushMethod}">
                            <Image Source="{StaticResource MeImage}"/>
                            <TextBlock VerticalAlignment="Center">Methods</TextBlock>
                        </StackPanel>
                    </ToggleButton>
                        <ToggleButton Name="toggleParameters" Click="toggleParameters_Click" >
                        <StackPanel Orientation="Horizontal" Background="{StaticResource BrushParameter}">
                            <Image Source="{StaticResource PaImage}"/>
                            <TextBlock VerticalAlignment="Center">Parameters</TextBlock>
                        </StackPanel>
                    </ToggleButton>
                        <ToggleButton Name="toggleVariables" Click="toggleVariables_Click">
                        <StackPanel Orientation="Horizontal" Background="{StaticResource BrushVariable}">
                            <Image Source="{StaticResource VaImage}"/>
                            <TextBlock VerticalAlignment="Center">Variables</TextBlock>
                        </StackPanel>
                    </ToggleButton>
                </StackPanel>

                    <!--<ListBox Name="lstBoxAnalyze" ItemsSource="{Binding Source=_analyzer.view}" SelectionMode="Multiple">-->
                    <ListBox Name="lstBoxAnalyze" ItemsSource="{Binding View}" SelectionMode="Multiple">
                    <ListBox.ItemTemplate>
                        <DataTemplate>
                            <StackPanel Orientation="Horizontal">
                                <ContentControl Margin="3" Width="20" Height="20" Content="{Binding Image}"/>
                                <TextBlock Text="{Binding Name}"/>
                            </StackPanel>
                        </DataTemplate>
                    </ListBox.ItemTemplate>

                    <ListBox.ItemContainerStyle>
                        <Style TargetType="{x:Type ListBoxItem}">
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding Path=Editible}" Value="False">
                                    <Setter Property="Focusable" Value="False" />
                                </DataTrigger>
                                <DataTrigger Binding="{Binding Path=Type}" Value="0">
                                    <Setter Property="Background" Value="{DynamicResource BrushAssembly}" />
                                </DataTrigger>
                                <DataTrigger Binding="{Binding Path=Type}" Value="1">
                                    <Setter Property="Background" Value="{DynamicResource BrushModule}" />
                                </DataTrigger>
                                <DataTrigger Binding="{Binding Path=Type}" Value="2">
                                    <Setter Property="Background" Value="{DynamicResource BrushType}" />
                                </DataTrigger>
                                <DataTrigger Binding="{Binding Path=Type}" Value="3">
                                    <Setter Property="Background" Value="{DynamicResource BrushMethod}" />
                                </DataTrigger>
                                <DataTrigger Binding="{Binding Path=Type}" Value="4">
                                    <Setter Property="Background" Value="{DynamicResource BrushParameter}" />
                                </DataTrigger>
                                <DataTrigger Binding="{Binding Path=Type}" Value="5">
                                    <Setter Property="Background" Value="{DynamicResource BrushVariable}" />
                                </DataTrigger>
                                <DataTrigger Binding="{Binding Path=Type}" Value="6">
                                    <Setter Property="Background" Value="{DynamicResource BrushRed}" />
                                </DataTrigger>
                            </Style.Triggers>

                        </Style>
                    </ListBox.ItemContainerStyle>
                </ListBox>
                </DockPanel>
            </Expander>

        </DockPanel>

    </Grid>
</Window>
